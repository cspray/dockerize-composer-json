{
  "name": "cspray/phocker-file",
  "description": "Generate a Dockerfile for running a PHP application based on your composer.json",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "",
      "role": "Project Maintainer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.21"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Phockerfile\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Phockerfile\\": "tests"
    }
  }
}